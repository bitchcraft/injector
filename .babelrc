{
    "presets": [
        "env",
        "stage-0",
        "flow",
        "react"
    ],
    "plugins": [
        "add-module-exports",
        "transform-runtime",
        [ "module-resolver", {
            "root": [ "./" ],
            "alias": {
                "src": "./src",
                "lib": "./lib"
            }
        } ]
    ]
}
